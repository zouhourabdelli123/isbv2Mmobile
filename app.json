{
  "expo": {
    "name": "ISB",
    "slug": "isb",
    "version": "2.0.4",
    "orientation": "portrait",
    "icon": "./assets/isb.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/isb.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/isb.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.exadev.isb",
      "permissions": [
        "RECEIVE_BOOT_COMPLETED",
        "VIBRATE",
        "POST_NOTIFICATIONS"
      ],
      "compileSdkVersion": 34,
      "targetSdkVersion": 34,
      "config": {
        "firebase": {
          "messaging": {
            "default_notification_color": "#000000"
          }
        }
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "notification": {
      "icon": "./assets/isb.png",
      "androidMode": "default",
      "androidCollapsedTitle": "IIT Notifications",
      "color": "#2196F3"
    },
    "plugins": [
      "react-native-iap",
      "@react-native-firebase/app",
      "@react-native-firebase/auth",
      "./plugins/withNotificationColorFix",
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          }
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "b5d44e4f-3b75-4298-bb6e-24b04cea79d4"
      }
    }
  }
}
