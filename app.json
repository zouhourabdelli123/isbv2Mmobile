    "web": {
      "favicon": "./assets/favicon.png"
    },
    "notification": {
      "icon": "./assets/isb.png",
      "androidMode": "default",
      "androidCollapsedTitle": "IIT Notifications",
      "color": "#2196F3"
    },

    "extra": {
      "eas": {
