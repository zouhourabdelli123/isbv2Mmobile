{
  "expo": {
    "name": "isb",
    "slug": "isb",
    "version": "1.0.4",
    "icon": "./assets/isbfont.png",
    "splash": {
      "image": "./assets/isbfont.png",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.exadev.isb",
      "infoPlist": {
        "UIBackgroundModes": [
          "remote-notification"
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/isbfont.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.exadev.isb",
      "permissions": [
        "RECEIVE_BOOT_COMPLETED",
        "VIBRATE",
        "POST_NOTIFICATIONS"
      ],
      "compileSdkVersion": 34,
      "targetSdkVersion": 34,
      "googleServicesFile": "./google-services.json",
      "config": {
        "firebase": {
          "messaging": {
            "default_notification_color": "#1e40af",
            "default_notification_channel_id": "isb_notifications"
          }
        }
      }
    },
    "web": {
      "favicon": "./assets/isb.png"
    },
    "notification": {
      "icon": "./assets/isbfont.png",
      "color": "#1e40af",
      "androidMode": "default",
      "androidCollapsedTitle": "ISB Notifications",
      "iosDisplayInForeground": true
    },
    "plugins": [
      "react-native-iap",
      "@react-native-firebase/app",
      "@react-native-firebase/auth",
      [
        "expo-notifications",
        {
          "icon": "./assets/isbfont.png",
          "color": "#1e40af",
          "sounds": [
            "./assets/notification-sound.wav"
          ]
        }
      ],
      "./plugins/withNotificationColorFix",
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          },
          "android": {
            "compileSdkVersion": 34,
            "targetSdkVersion": 34,
            "buildToolsVersion": "34.0.0"
          }
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "22754d56-6c31-479c-affb-36db38cfb870"
      }
    }
  }
}
